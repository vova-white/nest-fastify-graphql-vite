{
  "name": "nest-fastify-swagger-vite",
  "version": "0.0.1",
  "description": "NestJS Fastity Vite Starter",
  "author": "Vladimir Belyj",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "node ./vite.server.mjs",
    "build": "run-p type-check && vite build",
    "build:cjs": "run-p type-check && vite build -- --cjs",
    "type-check": "tsc --noEmit -p tsconfig.build.json --composite false",
    "prebuild": "rimraf dist",
    "build:nest": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest --dir ./src",
    "test:cov": "vitest --coverage",
    "test:e2e": "vitest --dir ./test"
  },
  "dependencies": {
    "@fastify/static": "^6.5.0",
    "@graphql-tools/utils": "^8.10.0",
    "@nestjs/apollo": "^10.0.22",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.11",
    "@nestjs/graphql": "^10.0.22",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-fastify": "^8.0.11",
    "@nestjs/swagger": "^6.0.5",
    "apollo-server-fastify": "^3.10.1",
    "apollo-server-plugin-base": "^3.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "fastify-static": "^4.7.0",
    "graphql": "^16.6.0",
    "graphql-query-complexity": "^0.11.0",
    "graphql-subscriptions": "^2.0.0",
    "mongoose": "^6.5.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@swc/core": "^1.2.242",
    "@swc/jest": "^0.2.22",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.7",
    "@types/node": "^18.7.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitest/coverage-c8": "^0.22.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup-plugin-multi-input": "^1.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "unplugin-swc": "^1.3.2",
    "vite": "^3.0.9",
    "vite-plugin-restart": "^0.2.0",
    "vitest": "^0.22.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}